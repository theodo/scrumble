{
  "initial": {
    "cors": {
      "params": {
        "origin": true,
        "credentials": true,
        "maxAge": 86400
      }
    }
  },
  "routes": {
    "loopback#rest": {
      "paths": ["${restApiRoot}"]
    }
  },
  "files": {
    "loopback#static": {
      "params": "$!../public"
    }
  },
  "final:after": {
    "errorhandler": {}
  }
}
