{
  "baseUri": "/",
  "restApiRoot": "/api/v1",
  "host": "0.0.0.0",
  "port": "8000",
  "aclErrorStatus": 403,
  "remoting": {
    "context": {
      "enableHttpContext": true
    },
    "rest": {
      "normalizeHttpPath": true,
      "xml": false
    },
    "json": {
      "strict": true,
      "limit": "100kb"
    },
    "urlencoded": {
      "extended": true,
      "limit": "100kb"
    },
    "cors": false,
    "errorHandler": {
      "disableStackTrace": true
    }
  },
  "legacyExplorer": false
}
